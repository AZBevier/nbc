1.2
log
@Change mpxmac location.
@
text
@#
# Makefile for date for running date on mpx1x
# Object files must be produced and then cataloged
# on MPX1.X using the LD.DATE command file.
#
CC = nbcc
SLIB = /system/lib
SYSBIN = /system/bin
CFLAGS = -C -1 -DMPX -DMPX1X -M$(SYSBIN)/m.mpxmac1x -P$(SYSBIN)/mpxpre1x
LBIN = ./mpx1x
INSTALL = cp
MOVE = mv
MAKEFILE = make.mpx1x

OBJS = date.o
HDRS =
SRCS= date.c
MAIN = $(OBJS)
SLD = ld.date1x
LD = LD.DATE
SMK = mk.date1x
MK = MK.DATE
SDOC = date.m
DOC = DATE.M

all:	$(SLD) $(SMK) $(SDOC) $(MAIN)

$(MAIN):	$(MAKEFILE) $(SRCS)
	$(SYSBIN)/$(CC) $(CFLAGS) $(SRCS)

clean:
	-rm -f *.o $(LBIN)/*.O

clobber: clean
	-rm -f $(LBIN)/DATE.O $(LBIN)/DATE.C $(LBIN)/$(LD)
	-rm -f $(LBIN)/$(MK) $(LBIN)/$(DOC)

install: all
	@@if [ -d $(LBIN) ]; then \
			echo "$(LBIN) already exits"; \
	else \
			mkdir $(LBIN); \
	fi;
	mpxblk <date.c >$(LBIN)/DATE.C
	mpxblk <$(SLD) >$(LBIN)/$(LD)
	mpxblk <$(SMK) >$(LBIN)/$(MK)
	mpxblk <$(SDOC) >$(LBIN)/$(DOC)
	$(MOVE) date.o $(LBIN)/DATE.O
	/bin/chmod 664 $(LBIN)/*.?

@


