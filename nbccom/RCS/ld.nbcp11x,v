text
@$DEFM
$PAGE 0
$JOB NBCCOM
$IFT FILE P1OBJ.O OBJSKIP
$FILEMGR
CREATE P1OBJ.O,DM,350
X
$DEFNAME OBJSKIP
$IFT FILE NBCP1MAP MAPSKIP
$FILEMGR
CREATE NBCP1MAP,DM,100
X
$DEFNAME MAPSKIP
$A2 IN=SYC
$A1 OT=P1OBJ.O
$COMPRESS
^(BIN)C.SRTE.O
FLOAT.O
MYATOF.O
CGRAM.O
CODE.O
COMM1.O
FIXCALL.O
LOCAL.O
OPTIM.O
PFTN.O
SCAN.O
STAB.O
TREES.O
VERSION.O
XDEFS.O
$NOTE  END OF COMPRESS
$OPTION 19 25
$A1 SLO=NBCP1MAP
$ALLOCATE 27000
$A1 SGO=P1OBJ.O
$A1 LIB=CLB,,U
$A1 DIR=CDR,,U
$CATALOG
FILES 8
BUFFERS 8
OPTION LOWER PROMPT
ENVIRONMENT ,,SHARED,MAP2048
CATALOG NBCP1
$IFF ABORT ENDJOB
$NOTE *** ERROR *** CATALOG OF NBCP1 FAILED
$DEFN ENDJOB
$EOJ
$PAGE 24
$ENDM
@
