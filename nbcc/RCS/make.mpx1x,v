1.4
log
@Update mpxmac location.
@
text
@#
# Makefile for nbcc for running nbcc on mpx1x
# Object files must be produced and then cataloged
# on MPX1.X using the LD.NBCC command file.
#
CC = nbcc
SLIB = /system/lib
SYSBIN = /system/bin
CFLAGS = -C -1 -DMPX -DMPX1X -M$(SYSBIN)/m.mpxmac1x -P$(SYSBIN)/mpxpre1x
LBIN = ./mpx1x
INSTALL = cp
MOVE = mv
MAKEFILE = make.mpx1x

OBJS = nbcc.o
HDRS =
SRCS= nbcc.c
MAIN = $(OBJS)
SLD = ld.nbcc1x
LD = LD.NBCC
SMK = mk.nbcc1x
MK = MK.NBCC

all:	$(SLD) $(SMK) $(MAIN)

$(MAIN):	$(MAKEFILE) $(SRCS)
	$(SYSBIN)/$(CC) $(CFLAGS) $(SRCS)

clean:
	-rm -f *.o $(LBIN)/*.O

clobber: clean
	-rm -f $(LBIN)/NBCC.O $(LBIN)/NBCC.C
	-rm -f $(LBIN)/$(MK) $(LBIN)/$(LD)

install: all
	mpxblk <nbcc.c >$(LBIN)/NBCC.C
	mpxblk <$(SLD) >$(LBIN)/$(LD)
	mpxblk <$(SMK) >$(LBIN)/$(MK)
	$(INSTALL) nbcc.o $(LBIN)/NBCC.O
	/bin/chmod 664 $(LBIN)/*.?

@


