OBJ3 = mpxini.o mpxio.o mpxsvc.o nice.o num.o numd.o pathfind.o pause.o \
	perror.o pipe.o pow.o printf.o putchar.o putenv.o puts.o putw.o	\
	qsort.o rand.o regerror.o rew.o rindex.o rmdirp.o sbrk.o scanf.o \
