	strnicmp.o strnlen.o strpbrk.o strpos.o strrchr.o strrpbrk.o strrpos.o	strspn.o

SRC3 = mpxini.c mpxio.c mpxsvc.c nice.c num.c numd.c pathfind.c pause.c \
