#ident	"@@(#)nbclib:errexit.c	1.1"

#include  "errmsg.h"

/*	This routine sets the exit(2) value for exit actions.
	It returns the previous value.
*/

int
errexit(e)
int	e;
{
    int	oe;

    oe = Err.exit;
    Err.exit = e;
    return  oe;
}
