@$DEFM
$PAGE 0
$JOB SH
$IFT FILE CCOBJ.O OBJSKIP
$FILEMGR
CREATE CCOBJ.O,DM,100
X
$DEFNAME OBJSKIP
$IFT FILE SHMAP MAPSKIP
$FILEMGR
CREATE SHMAP,DM,200
X
$DEFNAME MAPSKIP
$A2 IN=SYC
$A1 OT=CCOBJ.O
$COMPRESS
C.SRTE.O
SETBRK.O
BLOK.O
STAK.O
CMD.O
FAULT.O
MAIN.O
WORD.O
STRING.O
NAME.O
ARGS.O
XEC.O
SERVIC.O
ERROR.O
IO.O
PRINT.O
MACRO.O
EXPAND.O
CTYPE.O
MSG.O
TEST.O
DEFS.O
HASH.O
HASHSV.O
PWD.O
FUNC.O
ECHO.O
HOMEDR.O
$NOTE  END OF COMPRESS
$OPTION 19 25
$A1 SLO=SHMAP
$ALLOCATE 1B800
$A1 SGO=CCOBJ.O
$A1 LIB=CLB,,U
$A1 DIR=CDR,,U
$CATALOG
FILES 8
BUFFERS 8
OPTION LOWER PROMPT
ENVIRONMENT ,,SHARED,MAP2048
CATALOG SH
$IFF ABORT ENDJOB
$NOTE *** ERROR *** CATALOG OF SH FAILED
$DEFN ENDJOB
$EOJ
$PAGE 24
$ENDM
@
